<!-- Camera Permission -->
    <key>NSCameraUsageDescription</key>
    <string>This app needs camera access to measure your heart rate using photoplethysmography (PPG). Place your finger over the camera and flashlight for accurate readings.</string>
    
	<key>UIBackgroundModes</key>
	<array>
    <string>audio</string>
	</array>

    <!-- Bluetooth Permissions -->
    <key>NSBluetoothAlwaysUsageDescription</key>
    <string>This app needs Bluetooth access to connect to your smartwatch and sync health data.</string>
    
    <key>NSMicrophoneUsageDescription</key>
    <string>Microphone access is required for voice emotion analysis</string>

    <key>NSBluetoothPeripheralUsageDescription</key>
    <string>This app needs Bluetooth access to connect to your smartwatch and sync health data.</string>
    
    <!-- Location Permission (required for Bluetooth on iOS) -->
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>Location access is required for Bluetooth device discovery to connect to your smartwatch.</string>
    
